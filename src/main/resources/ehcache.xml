<?xml version="1.0" encoding="UTF-8"?>
<ehcache
xsi:noNamespaceSchemaLocation="ehcache.xsd"
updateCheck="true"
monitoring="autodetect"
dynamicConfig="true"
>
<diskStore path="java.io.tmpdir/ARSCache"/>

<cache name="sessions"
          maxBytesLocalHeap="1M"
      timeToLiveSeconds="300"
      memoryStoreEvictionPolicy="LRU">
 </cache>
 <cache name="questions"
           maxBytesLocalHeap="1M"
       timeToLiveSeconds="300"
       memoryStoreEvictionPolicy="LRU">
  </cache>
  <cache name="answers"
            maxBytesLocalHeap="1M"
        timeToLiveSeconds="300"
        memoryStoreEvictionPolicy="LRU">
   </cache>
   <cache name="meta"
             maxBytesLocalHeap="1M"
         timeToLiveSeconds="300"
         memoryStoreEvictionPolicy="LRU">
    </cache>
    <cache name="learningprogress"
              maxBytesLocalHeap="1M"
          timeToLiveSeconds="300"
          memoryStoreEvictionPolicy="LRU">
     </cache>
</ehcache>
